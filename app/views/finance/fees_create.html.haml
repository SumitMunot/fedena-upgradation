#content-header
  = image_tag("show_finance.png")
  %h1= t('create_fees')
  %h3= t('creates_master_fees_and_additional_fees')
  #app-back-button
    = back_button
#page-yield
  - unless flash[:notice].nil?
    %p.flash-msg= flash[:notice]
  .bread_crumb
    = link_to t('finance_text'), :controller => "finance", :action=>"index"
    »
    \#{link_to "#{t('fees_text')}", :controller => "finance", :action => "fees_index"} »
    \#{t('create_fees')}
  .box
    .link-box
      .link-heading= link_to "#{t('master_fees')}", :action => "master_fees"
      .link-descr= t('create_master_fees')
    .link-box
      .link-heading= link_to "#{t('additional_fees')}", :action => "additional_fees"
      .link-descr= t('create_additional_fees')
